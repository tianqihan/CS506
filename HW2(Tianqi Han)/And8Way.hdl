/**
 * 8-way And:
 * out = (in[0] and in[1] and ... and in[7])
 */

CHIP And8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
	And(a = in[7], b = in[6], out = and1);
	And(a = in[5], b = in[4], out = and2);
	And(a = in[3], b = in[2], out = and3);
	And(a = in[1], b = in[0], out = and4);
	And(a = and1, b = and2, out = and5);
	And(a = and3, b = and4, out = and6);
	And(a = and5, b = and6, out = out);
}
